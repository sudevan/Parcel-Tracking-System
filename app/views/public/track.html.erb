<section class="form-auth text-center">
  <%= form_with url: consignment_path, method: :get do |form| %>
    <h1 class="h3 mb-3 fw-normal">Tracking Id </h1>
   <%= form.text_field :id, autofocus: true, class: "form-control", placeholder: "Tracking ID", autocomplete: "off"  %>
    <%= form.submit "Search", class: "w-100 btn btn-lg btn-primary" %>
  <% end %>
</section>

